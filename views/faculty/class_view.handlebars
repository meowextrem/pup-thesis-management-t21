<div class="container">
  <div class="box">
    <div class="box-header with-border">
      <h2>Batch {{classData.batch}}</h2>
      <h4>Section {{classData.section}}</h4>
   
      <form action="/faculty/class/groups" method="post">
        <input type="hidden" value="{{classData.id}}" name="classId">
        <!--<input type="submit" class="btn"  name="manageGroups" value="Manage Groups">-->
      </form>
    </div>
    
  

 


  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>Student Number</th>
          <th>Name</th>
          <th>Email</th>
          <th>Phone</th>
        </tr>
      </thead>
      <tbody>
        {{#each studentsData}}
          <tr>
            <td>{{this.student_id}}</td>
            <td>{{this.first_name}} {{this.middle_name}} {{this.last_name}} {{this.suffix}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phone_number}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  </div>
</div>


